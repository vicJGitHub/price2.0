package com.hywa.pricepublish.representation;

import java.util.ArrayList;
import java.util.List;
import lombok.NoArgsConstructor;

@NoArgsConstructor
public class ProductMonitorReps {

    private String type;

    List<ProductMonitorRep> list;

    public ProductMonitorReps(List<ProductMonitorRep> list, String type) {
        this.setList(list);
        this.setType(type);
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public List<ProductMonitorRep> getList() {
        return list;
    }

    public void setList(List<ProductMonitorRep> list) {
        this.list = new ArrayList<>(list);
    }
}

package com.hywa.pricepublish.representation;

import java.util.ArrayList;
import java.util.List;

public class PriceMonitorReps {

    List<PriceMonitorRep> list;

    public PriceMonitorReps(List<PriceMonitorRep> priceMonitorReps) {
        this.setList(priceMonitorReps);
    }

    private void setList(List<PriceMonitorRep> priceMonitorReps) {
        this.list = new ArrayList<>(priceMonitorReps);
    }

    public List<PriceMonitorRep> getList() {
        return list;
    }
}

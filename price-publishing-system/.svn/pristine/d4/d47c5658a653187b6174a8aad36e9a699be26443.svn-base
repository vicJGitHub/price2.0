  <!--通过字典类型查询字典-->
  <select id="selectByDictType" resultMap="BaseResultMap" parameterType="java.lang.String">
    select d.id,d.name,d.code,d.select_url,d.unselect_url
    from dict d
    where d.dict_type_id = #{dictTypeId}
    and d.is_del = 0
    <if test="name !=null and name != ''">
      and d.name like concat('%',#{name},'%')
    </if>
  </select>
  <select id="selectProductType"
    resultType="com.hywa.pricepublish.representation.ProductSmallTypeRep">
    SELECT a.id,
    a.id smallTypeId,
    a.name smallType,
    b.id bigTypeId,
    b.name bigType
    FROM price_publish.dict a
    join (SELECT c.id,c.name FROM price_publish.dict c left join dict_type d on d.id =
    c.dict_type_id where d.code = "productType") b
    on a.dict_type_id =b.id
    WHERE
    a.is_del=0
    <if test="id != null and id != ''">
      and a.id=#{id}
    </if>
    <if test="name != null and name != ''">
      and a.name like concat('%',#{name},'%')
    </if>
    <if test="bigTypeId != null and bigTypeId != ''">
      and a.dict_type_id = #{bigTypeId}
    </if>
  </select>
  <select id="selectPriceType" resultType="com.hywa.pricepublish.dao.entity.Dict">
    select * from dict where dict_type_id = (select id from dict_type where code = 'priceType');
  </select>
